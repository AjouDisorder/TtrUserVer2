<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestActivity">

    <TextView
        android:id="@+id/percent"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="232dp"
        android:background="#999990"
        android:text="할인율"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/qunatity"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="168dp"
        android:background="#999990"
        android:text="수량"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="67dp"
        android:layout_height="25dp"
        android:layout_marginStart="272dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="292dp"
        android:background="#999990"
        android:text="4900"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="67dp"
        android:layout_height="25dp"
        android:layout_marginStart="272dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="232dp"
        android:background="#999990"
        android:text="17"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="25dp"
        android:layout_height="31dp"
        android:layout_marginStart="83dp"
        android:layout_marginBottom="292dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/searched_menu_disP"
        app:srcCompat="@drawable/logo_cloud" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="25dp"
        android:layout_height="31dp"
        android:layout_marginStart="83dp"
        android:layout_marginBottom="232dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/searched_menu_disP"
        app:srcCompat="@drawable/logo_cloud" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="67dp"
        android:layout_height="25dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="355dp"
        android:background="#999990"
        android:text="5600"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/lineline" />

    <TextView
        android:id="@+id/detailedMenuName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Macdonald"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/searched_menu_pic"
        android:layout_width="293dp"
        android:layout_height="194dp"
        android:src="@drawable/logo_home"
        app:layout_constraintBottom_toTopOf="@+id/searched_menu_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detailedMenuName" />

    <TextView
        android:id="@+id/searched_menu_originP"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_marginStart="37dp"
        android:layout_marginBottom="355dp"
        android:background="#999990"
        android:text="원가"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searched_menu_title" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="25dp"
        android:layout_height="31dp"
        android:layout_marginStart="83dp"
        android:layout_marginBottom="355dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/searched_menu_originP"
        app:layout_constraintTop_toBottomOf="@+id/searched_menu_title"
        app:srcCompat="@drawable/logo_cloud" />

    <TextView
        android:id="@+id/searched_menu_disP"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_marginStart="37dp"
        android:layout_marginBottom="295dp"
        android:text="특가"
        android:textAlignment="center"
        android:background="#999990"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="160dp"
        android:onClick="plusQ"
        android:text="+"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/qunatity" />

    <Button
        android:id="@+id/button3"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="160dp"
        android:onClick="minusQ"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button2" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="46dp"
        android:layout_height="0dp"
        android:layout_marginStart="270dp"
        android:layout_marginBottom="173dp"
        android:background="#999990"
        android:text="1"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="172dp"
        android:layout_height="67dp"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="35dp"
        android:text="이 가게 다른 메뉴 보기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/pay_button_test"
        android:layout_width="172dp"
        android:layout_height="67dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="35dp"
        android:text="식권 결제"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/searched_menu_title"
        android:layout_width="195dp"
        android:layout_height="30dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="115dp"
        android:layout_marginBottom="10dp"
        android:text="Bigmac"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searched_menu_pic"
        app:layout_constraintVertical_bias="0.0" />
    <View
        android:id="@+id/lineline"
        android:layout_width="match_parent"
        android:background="@android:color/darker_gray"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@id/searched_menu_title"
        android:layout_height="1dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"/>
    <TextView
        android:id="@+id/textView27"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="240dp"
        android:text="원"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/lineline"/>

    <TextView
        android:id="@+id/textView29"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="177dp"
        android:text="원"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toEndOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="119dp"
        android:text="%"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toEndOf="@+id/textView13" />
</androidx.constraintlayout.widget.ConstraintLayout>
